{
  "manifest_version": 3,
  "name": "Redmine Issue Updater",
  "version": "1.0",
  "description": "Update the status of Redmine issues using your API key.",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "https://redmine.linways.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
